/var/log/{{ subdir     }}debug.log			600	7	1000	*	C
/var/log/{{ subdir     }}devd.log			644	3	1000	*	C
/var/log/{{ subdir     }}init.log			644	3	1000	*	-
/var/log/{{ subdir     }}kerberos.log			600	7	1000	*	-
/var/log/{{ subdir     }}lpd-errs			644	7	1000	*	C
